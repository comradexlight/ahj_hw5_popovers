{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "no-new": 0,
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
    ]
  }
}
